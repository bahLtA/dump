Algoritmo Guia0_ejer2
	Definir imc, peso, estatura Como Real;
	Definir nombre Como Caracter;
	Escribir "Ingrese el nombre";
	Leer nombre;
	Escribir "Ingrese el peso (en Kg)";
	leer peso;
	Escribir "Ingrese la estatura (en Mts)";
	leer estatura;
	imc=peso/(estatura*estatura);
	Escribir nombre,", su IMC es de: ", imc;
	
	
FinAlgoritmo
