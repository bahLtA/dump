Algoritmo guia1_ejer5
	Definir Ginecologia,Traumatologia,Pediatria, PresupuestoTotal Como Real;
	Escribir "Ingrese el presupuesto total";
	Leer PresupuestoTotal;
	Ginecologia=PresupuestoTotal*0.4;
	Traumatologia=PresupuestoTotal*0.3;
	Pediatria=PresupuestoTotal*0.3;
	Escribir "El presupuesto de Ginecologia es de ",Ginecologia,"$";
	Escribir "El presupuesto de Traumatologia es de ", Traumatologia,"$";
	Escribir "El presupuesto de Pediatria es de ", Pediatria,"$";
FinAlgoritmo
